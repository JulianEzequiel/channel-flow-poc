<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.poc.flowchannel">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.poc.flowchannel.examples.ui.FlowActivity" />

        <activity android:name="com.poc.flowchannel.examples.ui.ChannelActivity" />

        <activity android:name="com.poc.flowchannel.examples.ui.StateFlowActivity" />

        <activity android:name="com.poc.flowchannel.examples.ui.ChannelReceiversActivity" />

        <activity android:name="com.poc.flowchannel.examples.ui.BroadcastChannelActivity" />

        <activity android:name=".app.home.ui.HomeActivity" />

        <activity android:name=".app.detail.ui.TweetDetailActivity" />

    </application>

</manifest>